{
  "name": "hub",
  "version": "0.0.1",
  "private": true,
  "hel": {},
  "main": "./src/index.ts",
  "scripts": {
    "start": "node ../../dev/scripts/start.js",
    "start:hel": "node ../../dev/scripts/hel/start.js",
    "start:hwl": "node ../../dev/scripts/hel/startWL.js",
    "start:hwr": "node ../../dev/scripts/hel/startWR.js",
    "start:all": "node ../../dev/scripts/hel/startAll.js",
    "start:deps": "node ../../dev/scripts/hel/startDeps.js",
    "build": "node ../../dev/scripts/build.js",
    "build:hel": "node ../../dev/scripts/hel/build.js",
    "build:all": "node ../../dev/scripts/hel/buildAll.js",
    "build:allm": "cross-env-shell \"pnpm run build:all && pnpm run build:meta\"",
    "build:helm": "cross-env-shell \"pnpm run build:hel && pnpm run build:meta\"",
    "build:meta": "node ../../dev/scripts/hel/meta",
    "test": "node ../../dev/scripts/test --config ../../dev/config/jest/jest.config.js",
    "test:once": "cross-env ONCE=1 node ../../dev/scripts/test --config ../../dev/config/jest/jest.config.js"
  },
  "dependencies": {
    "hel-mono-runtime-helper": "^1.0.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "volta": {
    "node": "18.12.1",
    "pnpm": "8.6.8"
  }
}
